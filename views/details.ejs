  <body>

    <%- include('./templatesTemp/navigation'); %>

    <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
      <h1 class="display-4">NGSIConnectorWEB</h1>
      <p class="lead">Here you can see details of chosen Entity.</p>
    </div>

    <div class="container">
            <% if(data.type == 'DepositPointType'){ %>
                <%- include('./templates/depositPointType', {element: data}); %>
             <% } %>
             <% if(data.type == 'DepositPoint'){ %>
                <%- include('./templates/depositPoint', {element: data}); %>
             <% } %>
             <% if(data.type == 'VehicleType'){ %>
                <%- include('./templates/vehicleType', {element: data}); %>
             <% } %>
             <% if(data.type == 'Transaction'){ %>
                <%- include('./templates/transaction', {element: data}); %>
             <% } %>
             <% if(data.type == 'SortingType'){ %>
                <%- include('./templates/sortingType', {element: data}); %>
             <% } %>
             <% if(data.type == 'Vehicle'){ %>
                <%- include('./templates/vehicles', {element: data}); %>
             <% } %>
             <% if(data.type == 'Waste'){ %>
                <%- include('./templates/waste', {element: data}); %>
             <% } %>
             <% if(data.type == 'WasteCategory'){ %>
                <%- include('./templates/wasteCategory', {element: data}); %>
             <% } %>
             <% if(data.type == 'DepositPointIsle'){ %>
                <%- include('./templates/depositPoint', {element: data}); %>
             <% } %>
      <footer class="pt-4 my-md-5 pt-md-5 border-top">
        <div class="row">
          <div class="col-12 col-md">
            <small class="d-block mb-3 text-muted">&copy; 2017-2018</small>
          </div>
