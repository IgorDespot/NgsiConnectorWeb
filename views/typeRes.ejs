<!doctype html>
<html lang="en">

    <head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">
  <link rel="icon" href="../../../../favicon.ico">

  <title>NGSI CONNECTOR</title>

  <!-- Bootstrap core CSS -->
  <link href="/stylesheets/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">

  <!-- Custom styles for this template -->
  <link href="/stylesheets/home.css" rel="stylesheet">
  <link href="/stylesheets/entity.css" rel="stylesheet">
</head>

  <body>

    <%- include('./templatesTemp/navigation'); %>

    <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
      <h1 class="display-4">NGSIConnectorWEB</h1>
      <p class="lead">Here you can quickly see all entities for given Fiware-Service and Fiware-Service-Path</p>
    </div>

    <div class="container">
            <div id="patka" class="table-responsive">
                <table class="table table-bordered table-hover w-auto h-auto">
                    <thead class="thead-light"> 
                        <tr>
                            <% for (var i = 0; i < keys.length; i++) { %>
                                <th scope="col"><%= keys[i] %></th>
                            <% } %>
                        </tr>
                    </thead>
                    <tbody>
                        <% for (var i = 0; i < data.length; i++) { %>
                            <tr >
                                <% for (var key in data[i]) { %>
                                    <%if (key == 'id' || key == 'type') { %>
                                        <td style="white-space: nowrap;width: 1%;">
                                            <%= data[i][key] %>
                                        </td>
                                    <% } else { %>
                                        <td style="white-space: nowrap;width: 1%;">
                                            <%= data[i][key].value %>
                                        </td>
                                    <% } %>
                                <% } %>
                            </tr>
                        <% } %>
                    </tbody>
                </table>     
            </div></br>
        <%if (message != 'undefind') { %>
            <div class="alert alert-success"  id="success-alert" role="alert">
                <strong>Well done!</strong> You successfully retrived data.
            </div>
        <% } %>
      <footer class="pt-4 my-md-5 pt-md-5 border-top">
        <div id="footer"  class="row">
          <div class="col-12 col-md">
            <small class="d-block mb-3 text-muted">&copy; 2017-2018</small>
          </div>
      </footer>
    </div>
    <%- include('./templatesTemp/scripts'); %>
  </body>
</html>
