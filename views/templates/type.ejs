<form method="POST" action="/indextype">
	<!-- COMPONENT START -->
	<div class="form-group">
		<label for="">Fiware-Service</label>
		<div class="input-group input-file" name="Fichier1">
			<input name="fiwareService" type="text" class="form-control" placeholder='Enter Fiware-Service...' />
		</div>
	</div>
	<div class="form-group">
		<label for="">Fiware-Service-Path</label>
		<div class="input-group input-file" name="Fichier1">
			<input name="fiwareServicePath" type="text" class="form-control" value=<%= data %>/>
		</div>
	</div>
	<div class="form-group">
		<label for="">Access-Token</label>
		<div class="input-group input-file" name="Fichier1">
				<%if (typeof token != 'undefined') { %>
					<input id="accessTokenUpload" name="accessToken" type="text" class="form-control test" placeholder='Enter Access-Token...'  value=<%= token %> />
				<% } else { %>
					<input id="accessTokenUpload" name="accessToken" type="text" class="form-control test" placeholder='Enter Access-Token...' />
				<% } %>
		</div>
	</div>
  <div class="form-group">
		<label for="">Entity-Type</label>
		<div class="input-group input-file" name="Fichier1">
			<input name="type" type="text" class="form-control" placeholder='Enter Entity Type' />
		</div>
	</div>
	<!-- COMPONENT END -->
	<div class="form-group">
			<button id="main" type="submit" class="btn btn-primary pull-right">Get Type</button>
			<button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
					Get Access Token
				</button>
		</div>
	<div class="collapse" id="collapseExample">
			<div class="card card-body">
				<form method="POST" action="/token">
						<div class="form-group">
								<label for="">Email</label>
								<div class="input-group input-file" name="Fichier1">
									<input name="username" type="text" class="form-control" placeholder='Enter Fiware-Service...' />
								</div>
							</div>
							<div class="form-group">
								<label for="">Password</label>
								<div class="input-group input-file" name="Fichier1">
									<input name="password" type="password" class="form-control" placeholder='Enter Fiware-Service-Path...' />
								</div>
							</div>
							<div class="form-group">
									<button id="main" type="submit" class="btn btn-primary pull-right">Send</button>
								</div>
				</form>
			</div>
		</div></br>
		<% if(typeof msg != 'undefined') { %>
			<div class="alert alert-warning"  id="success-alert" role="alert">
				<strong>Error!</strong> <%= msg %>
			</div>
		<% } %>
</form>
